{
  "name": "@meaningfully/electron-app",
  "version": "0.1.3",
  "description": "Semantic search over CSVs",
  "main": "./out/main/index.js",
  "author": "jeremybmerrill.com",
  "homepage": "https://github.com/jeremybmerrill/meaningfully",
  "type": "module",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && node scripts/ensureWeaviateIsDownloaded.mjs && electron-builder --win",
    "build:mac": "npm run build && node scripts/ensureWeaviateIsDownloaded.mjs && electron-builder --mac",
    "build:linux": "npm run build && node scripts/ensureWeaviateIsDownloaded.mjs && electron-builder --linux",
    "test": "vitest --silent=false --disable-console-intercept",
    "wdio": "NODE_ENV=test wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@llamaindex/azure": "^0.1.31",
    "@llamaindex/google": "^0.3.18",
    "@llamaindex/mistral": "^0.1.14",
    "@llamaindex/ollama": "^0.1.14",
    "@llamaindex/openai": "^0.4.14",
    "@llamaindex/postgres": "^0.0.62",
    "@llamaindex/weaviate": "^0.0.34",
    "better-sqlite3": "^11.8.1",
    "electron-updater": "^6.1.7",
    "js-tiktoken": "^1.0.8",
    "llamaindex": "^0.11.14",
    "lodash": "^4.17.21",
    "natural": "^8.1.0",
    "openai": "^5.9.0",
    "papaparse": "^5.5.3",
    "svelte": "^5.0.0",
    "svelte-routing": "^2.13.0",
    "weaviate-client": "^3.5.3",
    "weaviate-ts-embedded": "github:jeremybmerrill/typescript-embedded",
    "@meaningfully/core": "../core"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@tailwindcss/vite": "^4.0.6",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/lodash": "^4.17.17",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.14.8",
    "@wdio/cli": "^9.12.6",
    "@wdio/cucumber-framework": "^9.12.6",
    "@wdio/local-runner": "^9.12.6",
    "@wdio/spec-reporter": "^9.12.6",
    "bufferutil": "^4.0.9",
    "electron": "31.0.2",
    "electron-builder": "^24.13.3",
    "electron-notarize": "^1.2.2",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.45.1",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.6",
    "tslib": "^2.6.3",
    "typescript": "^5.7.3",
    "utf-8-validate": "^6.0.5",
    "vite": "^5.4.4",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8",
    "wdio-electron-service": "^8.1.0",
    "wdio-wait-for": "^3.1.0",
    "ws": "^8.18.3"
  }
}
